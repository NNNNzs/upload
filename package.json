{
  "name": "vite-upload",
  "version": "0.0.4",
  "author": {
    "name": "NNNNzs",
    "url": "https://github.com/NNNNzs"
  },
  "description": "基于vite，vue-core,ts,naive-ui，写的一个上传的页面，监听剪贴板的图片和链接地址",
  "keywords": [
    "vite",
    "typescript",
    "naive-ui"
  ],
  "scripts": {
    "dev": "vite",
    "ts":"vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "act --secret-file my.secrets"
  },
  "dependencies": {
    "axios": "0.25.0",
    "naive-ui": "2.24.1",
    "vue": "3.2.25",
    "vue-router": "4.0.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "vue-tsc --noEmit"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "2.0.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "less": "4.1.2",
    "typescript": "4.4.4",
    "vfonts": "0.0.3",
    "vite": "2.7.2",
    "vue-tsc": "0.29.8"
  }
}
